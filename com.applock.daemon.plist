<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE plist PUBLIC "-//Apple//DTD PLIST 1.0//EN" "http://www.apple.com/DTDs/PropertyList-1.0.dtd">
<plist version="1.0">
<dict>
    <key>Label</key>
    <string>com.applock.daemon</string>
    
    <key>ProgramArguments</key>
    <array>
        <string>PYTHON_PATH</string>
        <string>LOCKER_PATH</string>
    </array>
    
    <key>RunAtLoad</key>
    <true/>
    
    <key>KeepAlive</key>
    <true/>
    
    <key>StandardOutPath</key>
    <string>/tmp/applock.log</string>
    
    <key>StandardErrorPath</key>
    <string>/tmp/applock.error.log</string>
</dict>
</plist>
